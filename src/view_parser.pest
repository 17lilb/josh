WHITESPACE = _{ " " | "\t" }


view = { item* }

item = _{ transform }

transform = { "!" ~ cmd ~ "/" ~ name }

name = { (!("!" | ")" | "," | "\n") ~ ANY)+ }

cmd = { (!("/" | "(" | "," | "\n") ~ ANY)* }

viewfile = { "\n"? ~ file_entry ~ ("\n" ~ file_entry)* }

file_entry = { dst_path ~ "=" ~ view }

dst_path = @{ (!("=" | WHITESPACE) ~ ANY)+ }
